# Lichess OAuth Configuration
# Copy this file to .env and fill in your values

# Your Lichess OAuth App Client ID
# Get it from: https://lichess.org/account/oauth/app
LICHESS_CLIENT_ID=your_client_id_here

# Redirect URI (must match what's configured in Lichess OAuth app)
# For local development: http://localhost:4280/
# For production: https://your-app.azurestaticapps.net/
REDIRECT_URI=http://localhost:4280/
